{
  "name": "executable-verification",
  "version": "1.0.0",
  "description": "Executable verification scripts for research and planning phases",
  "private": true,
  "scripts": {
    "verify:research": "tsx research/assumptions.ts",
    "verify:plan": "tsx planning/preconditions.ts",
    "verify:all": "npm run verify:research && npm run verify:plan",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-morph": "^21.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "keywords": [
    "verification",
    "testing",
    "agentic-coding"
  ]
}
